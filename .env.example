# Local defaults for the JWebMP FullCalendar library (no secrets)
ENVIRONMENT=dev
SERVICE_NAME=fullcalendar-plugin
BASE_URL=http://localhost:8080
PORT=8080

# Auth placeholders (align with rules/generative/platform/secrets-config/env-variables.md)
OAUTH2_ISSUER_URL=https://auth.example.com/realms/dev
OAUTH2_REALM=dev
OAUTH2_CLIENT_ID=fullcalendar-client
OAUTH2_CLIENT_SECRET=change-me
JWKS_URI=https://auth.example.com/realms/dev/protocol/openid-connect/certs

# Observability and testing toggles
TRACING_ENABLED=false
LOGGING_BACKEND=log4j2
ENABLE_DEBUG_LOGS=false
SKIP_INTEGRATION_TESTS=true
TEST_DB_CONTAINER_IMAGE=postgres:16-alpine
CLOUDEVENTS_FORMAT=application/cloudevents+json
